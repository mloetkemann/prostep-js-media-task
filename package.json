{
  "name": "prostep-js-media-task",
  "version": "1.4.1",
  "description": "Media Tasks Library based on ffmpeg for prostepJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": "./dist/index.js",
    "./dist/tasks/scaleAudio": "./dist/tasks/scaleAudio.js",
    "./dist/tasks/tag": "./dist/tasks/tag.js",
    "./dist/tasks/getDurationTask": "./dist/tasks/getDurationTask.js"
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/index.d.ts"
      ],
      "scaleAudio.d.ts": [
        "dist/tasks/scaleAudio.d.ts"
      ]
    }
  },
  "type": "module",
  "scripts": {
    "build": "pnpm lint && tsc -p .",
    "test": "npx mocha",
    "lint": "eslint .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^5.1.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.4.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "dependencies": {
    "alpha8-lib": "^1.4.11",
    "execa": "^8.0.1",
    "prostep-js": "^1.5.4"
  }
}
